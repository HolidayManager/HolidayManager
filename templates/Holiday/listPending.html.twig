{% for holiday in pending %}
    <div class="pendingRow">
        <div>{{ holiday.getUser().getFirstName() }}</div>
        <div>{{ holiday.getUser().getLastname() }}</div>
        <div>{{ holiday.getStartDate() | date }}</div>
        <div>{{ holiday.getEndDate() | date }}</div>
        <div class="idaccept"><span class="accept" href="/holiday/accept/{{ holiday.getId() }}" id="{{ holiday.getId() }}">Accept</span></div>
        <div><span class="refuse" href="/holiday/refuse/{{ holiday.getId() }}">Refuse</span></div>
    </div>
{% endfor %}


